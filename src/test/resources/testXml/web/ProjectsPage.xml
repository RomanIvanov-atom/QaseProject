<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Projects page">
    <test name="#5 Test create project">
        <classes>
            <class name="tests.ProjectsPageTest">
                <methods>
                    <include name="createProjectTest"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="#6 Test update project">
        <classes>
            <class name="tests.ProjectsPageTest">
                <methods>
                    <include name="updateProjectTest"/>
